   <string notr="true">/* ============================================================
   Global Styles - SCADA Dark Theme
   ============================================================ */
QMainWindow {
    background-color: #1E1E1E;
}

QWidget {
    color: #E0E0E0;
    font-family: 'Segoe UI', 'Noto Sans KR', sans-serif;
    font-size: 12px;
}

/* ============================================================
   Scroll Bar
   ============================================================ */
QScrollBar:vertical {
    border: none;
    background: #2D2D30;
    width: 10px;
    margin: 0;
}

QScrollBar::handle:vertical {
    background: #555555;
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: #666666;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    border: none;
    background: #2D2D30;
    height: 10px;
    margin: 0;
}

QScrollBar::handle:horizontal {
    background: #555555;
    border-radius: 5px;
    min-width: 30px;
}

/* ============================================================
   Section GroupBox (Main Sections) - Dashed Border Style
   ============================================================ */
QGroupBox#sectionGroup {
    background-color: transparent;
    border: 1px dashed #555555;
    border-radius: 0px;
    margin-top: 8px;
    padding: 10px;
    padding-top: 5px;
    font-weight: normal;
}

QGroupBox#sectionGroup::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 5px;
    top: 0px;
    padding: 0px 3px;
    background-color: #1E1E1E;
    color: #888888;
    font-size: 11px;
    font-weight: normal;
}

/* Inner GroupBox (Statistics, Save, Preview) - Dashed Border Style */
QGroupBox#innerGroup {
    background-color: transparent;
    border: 1px dashed #555555;
    border-radius: 0px;
    margin-top: 8px;
    padding: 8px;
    padding-top: 5px;
}

QGroupBox#innerGroup::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 5px;
    top: 0px;
    padding: 0px 3px;
    background-color: #1E1E1E;
    color: #888888;
    font-size: 11px;
    font-weight: normal;
}

/* ============================================================
   Buttons
   ============================================================ */
QPushButton {
    background-color: #3C3C3C;
    color: #E0E0E0;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 4px 12px;
    font-weight: 500;
}

QPushButton:hover {
    background-color: #505050;
    border-color: #007ACC;
}

QPushButton:pressed {
    background-color: #007ACC;
}

QPushButton:disabled {
    background-color: #2D2D30;
    color: #666666;
    border-color: #3C3C3C;
}

/* Action Button (Load File) */
QPushButton#actionButton {
    background-color: #007ACC;
    color: white;
    border: none;
    font-weight: bold;
    padding: 4px 12px;
}

QPushButton#actionButton:hover {
    background-color: #1E90FF;
}

/* Secondary Button */
QPushButton#secondaryButton {
    background-color: #3C3C3C;
    color: #E0E0E0;
    border: 1px solid #555555;
    padding: 4px 12px;
}

QPushButton#secondaryButton:hover {
    background-color: #505050;
    border-color: #007ACC;
}

/* Small Secondary Button */
QPushButton#smallSecondaryButton {
    background-color: #3C3C3C;
    color: #E0E0E0;
    border: 1px solid #555555;
    padding: 3px 10px;
    font-size: 11px;
}

QPushButton#smallSecondaryButton:hover {
    background-color: #505050;
    border-color: #007ACC;
}

/* Execute Button (Green) */
QPushButton#executeButton {
    background-color: #2E7D32;
    color: white;
    border: none;
    font-weight: bold;
    padding: 4px 12px;
}

QPushButton#executeButton:hover {
    background-color: #388E3C;
}

QPushButton#executeButton:pressed {
    background-color: #1B5E20;
}

/* Small Info Button */
QPushButton#infoButton {
    background-color: #555555;
    color: #E0E0E0;
    border: none;
    border-radius: 9px;
    padding: 0px;
    min-height: 18px;
    max-height: 18px;
    min-width: 18px;
    max-width: 18px;
    font-weight: bold;
    font-size: 11px;
}

QPushButton#infoButton:hover {
    background-color: #007ACC;
}

/* ============================================================
   Input Fields
   ============================================================ */
QLineEdit, QSpinBox, QDoubleSpinBox {
    background-color: #333337;
    border: 1px solid #3C3C3C;
    border-radius: 4px;
    padding: 4px 8px;
    color: #E0E0E0;
    selection-background-color: #007ACC;
}

QLineEdit:focus, QSpinBox:focus, QDoubleSpinBox:focus {
    border: 1px solid #007ACC;
}

QLineEdit:read-only {
    background-color: #2D2D30;
    color: #AAAAAA;
}

/* ComboBox */
QComboBox {
    background-color: #333337;
    border: 1px solid #3C3C3C;
    border-radius: 4px;
    padding: 4px 8px;
    color: #E0E0E0;
}

QComboBox:focus {
    border: 1px solid #007ACC;
}

QComboBox::drop-down {
    border: none;
    width: 20px;
}

QComboBox::down-arrow {
    image: none;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 5px solid #E0E0E0;
    margin-right: 6px;
}

QComboBox QAbstractItemView {
    background-color: #333337;
    border: 1px solid #3C3C3C;
    selection-background-color: #007ACC;
    color: #E0E0E0;
}

/* ============================================================
   Labels
   ============================================================ */
QLabel {
    color: #CCCCCC;
}

QLabel#infoLabel {
    color: #00CED1;
    font-weight: bold;
    font-size: 12px;
}

QLabel#subsectionTitle {
    color: #E0E0E0;
    font-weight: bold;
    font-size: 12px;
    padding-bottom: 5px;
}

QLabel#tableHeader {
    color: #AAAAAA;
    font-size: 11px;
}

/* ============================================================
   Tables
   ============================================================ */
QTableWidget {
    background-color: #1E1E1E;
    border: 1px solid #3C3C3C;
    gridline-color: #3C3C3C;
    color: #E0E0E0;
}

QTableWidget#dataTable {
    background-color: #252526;
}

QTableWidget#statsTable {
    background-color: #252526;
}

QTableWidget#selectionTable {
    background-color: transparent;
    border: none;
}

QTableWidget::item {
    padding: 5px;
    border: none;
}

QTableWidget::item:selected {
    background-color: #007ACC;
    color: white;
}

QHeaderView::section {
    background-color: #333337;
    color: #E0E0E0;
    padding: 6px;
    border: none;
    border-bottom: 1px solid #3C3C3C;
    border-right: 1px solid #3C3C3C;
    font-weight: bold;
}

/* ============================================================
   Checkbox
   ============================================================ */
QCheckBox {
    color: #E0E0E0;
    spacing: 8px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #555555;
    border-radius: 3px;
    background-color: #333337;
}

QCheckBox::indicator:checked {
    background-color: #007ACC;
    border-color: #007ACC;
}

QCheckBox::indicator:hover {
    border-color: #007ACC;
}

/* ============================================================
   Status Bar
   ============================================================ */
QStatusBar {
    background-color: #007ACC;
    color: white;
    padding: 5px;
    font-size: 12px;
}

/* ============================================================
   Frame
   ============================================================ */
QFrame {
    border: none;
}</string>
